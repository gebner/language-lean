{
  "name": "language-lean",
  "main": "./lib/main",
  "version": "0.2.2",
  "description": "Atom support for the Lean language",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "language-lean:toggle"
  },
  "activationHooks": [
      "language-lean:grammar-used"
  ],
  "repository": "https://github.com/jroesch/language-lean",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
      "linter-indie": {
          "versions": {
              "1.0.0": "consumeLinter"
          }
      }
  },
  "dependencies": {
      "atom-package-deps": "4.3.1"
  },
  "devDependencies": {
      "babel-core": "5.8.38",
      "babel-eslint": "6.1.2"
  },
  "package-deps" : [
      "linter",
      "latex-completions"
  ]
}
